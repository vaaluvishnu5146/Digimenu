<%- include('../partials/head');%>
<div class="container error-container">
  <div class="card error-card text-center p-5">
    <div class="card-body">
      <h1 class="display-1 fw-bold text-danger">401</h1>
      <h2 class="h4 mb-3">Unauthorized Access</h2>
      <p class="text-muted mb-4">
        Sorry, you do not have permission to view this directory or page using
        the credentials that you supplied.
      </p>
      <div class="d-grid gap-2">
        <a href="/dashboard" class="btn btn-primary">Return to Homepage</a>
        <a href="/" class="btn btn-outline-secondary"
          >Login with Different Account</a
        >
      </div>
    </div>
    <div class="card-footer bg-transparent border-0 text-muted">
      <small
        >Need help?
        <a href="#" class="text-decoration-none">Contact Support</a></small
      >
    </div>
  </div>
</div>
